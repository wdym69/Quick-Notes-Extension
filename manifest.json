{
  "manifest_version": 2,
  "name": "Quick Notes",
  "version": "1.0",
  "description": "A simple quick notes extension",
  "permissions": ["activeTab", "storage"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/logo-16.png",
      "48": "images/logo-48.png",
      "128": "images/logo-128.png"
    }
  },
  "icons": {
    "16": "images/logo-16.png",
    "48": "images/logo-48.png",
    "128": "images/logo-128.png"
  }
}
